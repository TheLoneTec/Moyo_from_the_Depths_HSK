<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<AlienRace.ThingDef_AlienRace ParentName="BasePawn">
		<defName>Alien_Moyo</defName>
		<label>Moyo</label>
		<description>A humanoid race living in cities under the sea. Having traits similar to sea creatures, their blood contains substances that can be used for various medical uses.\n\nDescendants of glitterworld explorers who settled in the deep sea, generations of body modifications has led to their current form. Spreading far and wide into the rim, they built many cities and prospered. Until now.</description>
		<descriptionHyperlinks>
			<ThingDef>Moyo_BloodBag</ThingDef>
		</descriptionHyperlinks>
		<alienRace>
			<generalSettings>
				<maleGenderProbability>0.3</maleGenderProbability>
				<immuneToAge>true</immuneToAge>
				<humanRecipeImport>true</humanRecipeImport>
				<canLayDown>true</canLayDown>
				<maxDamageForSocialfight>8</maxDamageForSocialfight>
				<immuneToXenophobia>false</immuneToXenophobia>
				<disallowedTraits>
					<li>
						<defName>Pyromaniac</defName>
					</li>
					<li>
						<defName>CreepyBreathing</defName>
					</li>
				</disallowedTraits>
				<chemicalSettings>
					<li>
						<chemical>DeepBlue</chemical>
						<reactions>
							<li Class="IngestionOutcomeDoer_GiveHediff">
								<hediffDef>Moyo_DeepBlueReaction</hediffDef>
								<severity>0.23</severity>
							</li>
						</reactions>
					</li>
					<li>
						<chemical>BlueBurstC</chemical>
						<reactions>
							<li Class="IngestionOutcomeDoer_GiveHediff">
								<hediffDef>BlueBurstHighPrecursor</hediffDef>
								<severity>0.01</severity>
							</li>
						</reactions>
						<ingestible>false</ingestible>
					</li>
				</chemicalSettings>
				<!--=파트제네레이터==========================================================================================-->
				<alienPartGenerator>
					<atlasScale>4</atlasScale>
					<borderScale>1</borderScale>
					<aliencrowntypes>
						<li>MoyoHead</li>
						<li>MoyoHead1</li>
						<li>MoyoHead2</li>
						<li>MoyoHead3</li>
						<li>MoyoHead4</li>
						<li>MoyoHead5</li>
						<li>MoyoHead6</li>
						<li>MoyoHead7</li>
					</aliencrowntypes>
					<alienbodytypes>
						<li>Female</li>
						<li>Thin</li>
					</alienbodytypes>
					<headOffset>(-0.07, -0.06)</headOffset>
					<useGenderedHeads>false</useGenderedHeads>
					<useGenderedBodies>false</useGenderedBodies>
					<!-- 신체 드로잉 크기 -->
					<customDrawSize>(1.0, 1.0)</customDrawSize>
					<customHeadDrawSize>(1.0, 1.0)</customHeadDrawSize>
					<customPortraitDrawSize>(1.0, 1.0)</customPortraitDrawSize>
					<bodyAddons>
						<!-- 꼬리 -->
						<li>
							<path>Moyo/Tail/MoyoTail</path>
							<bodyPart>tail</bodyPart>
							<shaderType>Cutout</shaderType>
							<inFrontOfBody>false</inFrontOfBody>
							<colorChannel>skin</colorChannel>
							<drawnInBed>false</drawnInBed>
							<defaultOffset>Tail</defaultOffset>
							<offsets>
								<south>
									<layerOffset>-0.27</layerOffset>
									<bodyTypes>
										<Female>(0,0)</Female>
										<Thin>(0,0)</Thin>
									</bodyTypes>
								</south>
								<north>
									<layerOffset>-0.28</layerOffset>
									<bodyTypes>
										<Female>(0,0.551)</Female>
										<Thin>(0,0.551)</Thin>
									</bodyTypes>
								</north>
								<east>
									<layerOffset>-0.28</layerOffset>
									<bodyTypes>
										<Female>(-0.34,0.193)</Female>
										<Thin>(-0.34,0.193)</Thin>
									</bodyTypes>
								</east>
							</offsets>
						</li>
						<!--더듬이-->
						<li>
							<path>Moyo/Heads/MoyoAtenna_Left</path>
							<bodyPart>left antennae</bodyPart>
							<inFrontOfBody>true</inFrontOfBody>
							<colorChannel>skin</colorChannel>
							<alignWithHead>true</alignWithHead>
							<offsets>
								<north>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</north>
								<south>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</south>
								<east>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</east>
							</offsets>
							<hiddenUnderApparelFor>
								<li>FullHead</li>
							</hiddenUnderApparelFor>
							<hiddenUnderApparelTag>
								<li>MoyoTuque</li>
							</hiddenUnderApparelTag>
						</li>
						<li>
							<path>Moyo/Heads/MoyoAtenna_Right</path>
							<bodyPart>right antennae</bodyPart>
							<inFrontOfBody>true</inFrontOfBody>
							<colorChannel>skin</colorChannel>
							<alignWithHead>true</alignWithHead>
							<offsets>
								<north>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</north>
								<south>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</south>
								<east>
									<bodyTypes>
										<Female>(0.0, 0.0)</Female>
										<Thin>(0.0, 0.0)</Thin>
									</bodyTypes>
								</east>
							</offsets>
							<hiddenUnderApparelFor>
								<li>FullHead</li>
							</hiddenUnderApparelFor>
							<hiddenUnderApparelTag>
								<li>MoyoTuque</li>
							</hiddenUnderApparelTag>
						</li>
					</bodyAddons>
					<!-- 컬러채널 -->
					<colorChannels>
						<li>
							<name>skin</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>10</weight>
										<only>(255,255,255)</only>
									</li>
								</options>
							</first>
						</li>
						<li>
							<name>hair</name>
							<first Class="ColorGenerator_Options">
								<options>
									<li>
										<weight>100</weight>
										<only>(56,55,78)</only>
									</li>
								</options>
							</first>
						</li>
					</colorChannels>
				</alienPartGenerator>
			</generalSettings>
			<graphicPaths>
				<li>
					<head>Moyo/Heads/</head>
					<body>Moyo/Body/</body>
					<skeleton>Things/Pawn/Humanlike/Bodies/Dessicated/Dessicated_Thin</skeleton>
				</li>
			</graphicPaths>
			<thoughtSettings>
				<cannotReceiveThoughts>
					<li>SoakingWet</li>
				</cannotReceiveThoughts>
			</thoughtSettings>
			<styleSettings>
				<li>
					<key>HairDef</key>
					<value>
						<hasStyle>true</hasStyle>
						<styleTagsOverride>
							<li>Moyo_HairCategory</li>
						</styleTagsOverride>
					</value>
				</li>
				<li>
					<key>TattooDef</key>
					<value>
						<hasStyle>false</hasStyle>
						<!-- <styleTags> -->
						<!-- <li>Minimal</li> -->
						<!-- </styleTags> -->
					</value>
				</li>
				<li>
					<key>BeardDef</key>
					<value>
						<hasStyle>false</hasStyle>
						<!-- <styleTags> -->
						<!-- <li>Minimal</li> -->
						<!-- </styleTags> -->
					</value>
				</li>
			</styleSettings>
			<raceRestriction>
				<onlyUseRaceRestrictedApparel>false</onlyUseRaceRestrictedApparel>
				<apparelList>
					<li>Moyo_Tribal</li>
					<li>Moyo_Parka</li>
					<li>Moyo_Jacket</li>
					<li>Moyo_Duster</li>
					<li>Moyo_SnB</li>
					<li>Moyo_RoyalRobeA</li>
					<li>Moyo_RoyalRobeB</li>
					<li>Moyo_RoyalRobeC</li>
					<li>Moyo_RoyalBlackDress</li>
					<li>Moyo_Tuque</li>
					<li>Moyo_BowlerHat</li>
					<li>Moyo_CowboyHat</li>
					<li>Moyo_RoyalHologramA</li>
					<li>Moyo_RoyalHologramB</li>
					<li>Moyo_RoyalHologramC</li>
					<li>Moyo_EGsuit</li>
					<li>Moyo_EGmask</li>
					<li>Moyo_SShelmet</li>
					<li>Moyo_PShelmet</li>
					<li>Moyo_LDgear</li>
					<li>Moyo_LDhelmet</li>
					<li>Moyo_DDgear</li>
					<li>Moyo_DDhelmet</li>
					<li>Moyo_GSsuit</li>
					<li>Moyo_GShelmet</li>
					<li>Moyo_GDsuit</li>
					<li>Moyo_GDhelmet</li>
					<li>Moyo_Evasuit</li>
					<li>Moyo_Bodysuit</li>
					<li>Moyo_Royalsuit</li>
					<li>Moyo_HoloJacket</li>
					<li>Moyo_Holosuit</li>
				</apparelList>
			</raceRestriction>
		</alienRace>
		<!--=============================================================================================================================-->
		<statBases>
			<!--기본-->
			<MarketValue>3200</MarketValue>
			<Flammability>0.5</Flammability>
			<MoveSpeed>4</MoveSpeed>
			<ComfyTemperatureMax>18</ComfyTemperatureMax>
			<ComfyTemperatureMin>2</ComfyTemperatureMin>
			<ImmunityGainSpeed>1.1</ImmunityGainSpeed>
			<CarryingCapacity>75</CarryingCapacity>
			<MeatAmount>80</MeatAmount>
			<LeatherAmount>50</LeatherAmount>
			<Mass>75</Mass>
			<PsychicSensitivity>1.2</PsychicSensitivity>
			<MeleeDodgeChance>0.8</MeleeDodgeChance>
			<SocialImpact>1</SocialImpact>
			<GeneralLaborSpeed>0.85</GeneralLaborSpeed>
			<DrugSynthesisSpeed>1.3</DrugSynthesisSpeed>
			<MedicalTendSpeed>1.2</MedicalTendSpeed>
			<MedicalTendQuality>1.15</MedicalTendQuality>
			<MedicalSurgerySuccessChance>1.15</MedicalSurgerySuccessChance>
			<RoyalFavorValue>4</RoyalFavorValue>
		</statBases>
		<!--==맨손전투===================================================================================================================-->
		<tools>
			<li>
				<label>left fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>8.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right fist</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>8.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>teeth</label>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>8.2</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.07</chanceFactor>
				<soundMeleeHit>Pawn_Melee_HumanBite_Hit</soundMeleeHit>
				<soundMeleeMiss>Pawn_Melee_HumanBite_Miss</soundMeleeMiss>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>5</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
		<!--=============================================================================================================================-->
		<race>
			<thinkTreeMain>Humanlike</thinkTreeMain>
			<thinkTreeConstant>HumanlikeConstant</thinkTreeConstant>
			<intelligence>Humanlike</intelligence>
			<makesFootprints>true</makesFootprints>
			<lifeExpectancy>100</lifeExpectancy>
			<!-- Gore -->
			<leatherDef>Leather_Human</leatherDef>
			<bloodDef>Moyo_Blood</bloodDef>
			<fleshType>Moyo_Flesh</fleshType>
			<meatMarketValue>0.8</meatMarketValue>
			<meatColor>(53, 89, 180)</meatColor>
			<!-- Gore -->
			<nameCategory>HumanStandard</nameCategory>
			<hasGenders>true</hasGenders>
			<body>Moyo</body>
			<baseHungerRate>0.8</baseHungerRate>
			<baseBodySize>1</baseBodySize>
			<baseHealthScale>1.2</baseHealthScale>
			<foodType>OmnivoreHuman</foodType>
			<gestationPeriodDays>30</gestationPeriodDays>
			<litterSizeCurve>
				<points>
					<li>(0, 0)</li>
					<li>(1, 10)</li>
					<li>(2, 1)</li>
					<li>(3, 0)</li>
				</points>
			</litterSizeCurve>
			<lifeStageAges>
				<li>
					<def>HumanlikeBaby</def>
					<minAge>0</minAge>
				</li>
				<li>
					<def>HumanlikeToddler</def>
					<minAge>1.2</minAge>
				</li>
				<li>
					<def>HumanlikeChild</def>
					<minAge>4</minAge>
				</li>
				<li>
					<def>HumanlikeTeenager</def>
					<minAge>12</minAge>
				</li>
				<li>
					<def>HumanlikeAdult</def>
					<minAge>18</minAge>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_Punch_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_Punch_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_Punch_Miss</soundMeleeMiss>
			<specialShadowData>
				<volume>(0.3, 0.8, 0.4)</volume>
				<offset>(0,0,-0.3)</offset>
			</specialShadowData>
			<ageGenerationCurve>
				<points>
					<li>(14,0)</li>
					<li>(16,60)</li>
					<li>(22,100)</li>
					<li>(30,30)</li>
					<li>(60,5)</li>
					<li>(80,1)</li>
					<li>(100,0)</li>
				</points>
			</ageGenerationCurve>
			<hediffGiverSets>
				<li>MoyoInate</li>
				<li>Human</li>
			</hediffGiverSets>
		</race>
		<!--=============================================================================================================================-->
		<comps>
			<li Class="HPF_Moyo.CompProperties_ResourceHarvestable">
				<harvestJobDef>ExtractDeepBlue_Job</harvestJobDef>
				<saveUniqueKey>HPFDeepBlueFullness</saveUniqueKey>
				<thingDef>Moyo_BloodBag</thingDef>
				<intervalDays>6.666666667</intervalDays>
				<amount>1</amount>
				<inspectText>Moyo BloodBag</inspectText>
				<constraints>
					<li Class="HPF_Moyo.ConstraintPawnAgeOver">
						<age>7</age>
					</li>
				</constraints>
				<productAffectedStats>
					<li>
						<statDef>ImmunityGainSpeed</statDef>
						<multiplier>2.0</multiplier>
						<offset>-1.0</offset>
					</li>
					<li>
						<statDef>DeepblueStat</statDef>
						<multiplier>1.0</multiplier>
						<offset>-1.0</offset>
					</li>
				</productAffectedStats>
				<speedAffectedStats>
					<li>
						<statDef>DeepblueStatSpeed</statDef>
						<multiplier>1.0</multiplier>
						<offset>-1.0</offset>
					</li>
				</speedAffectedStats>				
			</li>
		</comps>
		<!--=============================================================================================================================-->
		<recipes>
			<li>ExciseCarcinoma</li>
			<li>AdministerMechSerumHealer</li>
			<li>RemoveBodyPart</li>
			<li>Euthanize</li>
			<li>Anesthetize</li>
			<li>CureScaria</li>
			<li MayRequire="Ludeon.RimWorld.Royalty">CureBloodRot</li>
			<li MayRequire="Ludeon.RimWorld.Royalty">CureAbasia</li>
		</recipes>
	</AlienRace.ThingDef_AlienRace>
</Defs>